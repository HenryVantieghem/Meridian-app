{
  "name": "super-intelligence-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest --run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "test:unit": "vitest --run src/lib",
    "test:components": "vitest --run src/components",
    "test:api": "vitest --run src/app/api",
    "test:accessibility": "vitest --run accessibility",
    "test:performance": "vitest --run performance",
    "test:ai": "vitest --run ai",
    "test:integration": "vitest --run integration",
    "test:all": "npm run test:ci && npm run test:e2e",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npm run build && npm run analyze:bundle",
    "analyze:bundle": "npx @next/bundle-analyzer .next/analyze",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "lighthouse:ci": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "typegen": "supabase gen types typescript --project-id your-project-id > src/types/database.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "tsx src/scripts/seed.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@azure/identity": "^4.10.1",
    "@azure/msal-node": "^2.0.0",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@clerk/nextjs": "^6.24.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.3",
    "@slack/oauth": "^2.6.0",
    "@slack/socket-mode": "^1.3.0",
    "@slack/web-api": "^7.0.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.82.0",
    "@types/ws": "^8.18.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.3",
    "googleapis": "^128.0.0",
    "lucide-react": "^0.344.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "openai": "^4.24.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "resend": "^3.1.0",
    "stripe": "^14.17.0",
    "svix": "^1.69.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@next/bundle-analyzer": "^15.0.0",
    "@playwright/test": "^1.40.1",
    "@sentry/nextjs": "^9.36.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@upstash/redis": "^1.28.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.2.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.1.5",
    "node-cron": "^3.0.3",
    "node-mocks-http": "^1.12.1",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "puppeteer": "^21.0.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
