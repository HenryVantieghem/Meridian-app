{
  "name": "napoleon-ai-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:parallel": "concurrently \"npm run dev\" \"npm run test:watch\" \"npm run lint:watch\"",
    "build": "next build",
    "build:fast": "next build --no-lint",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "nodemon --exec \"npm run lint\" --ext ts,tsx,js,jsx",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "lighthouse:ci": "lhci autorun",
    "performance": "npm run build && npm run lighthouse",
    "bundle-analyze": "npm run build:analyze",
    "hot-reload": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"npm run dev\"",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "clean:all": "rm -rf .next out dist node_modules package-lock.json && npm install",
    "setup": "npm install && npm run type-check && npm run lint && npm run test:run && npm run build",
    "deploy:check": "npm run lint && npm run test:ci && npm run build && npm run lighthouse:ci",
    "deploy:prep": "npm run clean && npm run build:fast",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "pre-commit": "npm run lint && npm run test:run && npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@azure/identity": "^4.10.2",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@clerk/nextjs": "^5.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^9.38.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.3",
    "@slack/logger": "^4.0.0",
    "@slack/socket-mode": "^2.0.4",
    "@slack/web-api": "^7.9.3",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/forms": "^0.5.7",
    "@upstash/redis": "^1.35.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.16",
    "googleapis": "^152.0.0",
    "lucide-react": "^0.294.0",
    "next": "15.3.5",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hotkeys-hook": "^4.4.1",
    "resend": "^2.0.0",
    "stripe": "^14.12.0",
    "svix": "^1.69.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "github-repo-mcp": "^1.0.3",
    "husky": "^8.0.3",
    "lighthouse": "^11.6.0",
    "mcp-server-commands": "^0.5.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "serena": "^0.0.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.2.4"
}
